{{#each posts as |post|}}
  <a href="/post/{{id}}">
    <div class="row justify-space-between-md card">
      <h2 class="page-title card-header">{{title}}</h2>
      <p class="card-body">{{postContent}}</p>
      <p class="card-body">Posted by {{user.username}} on {{format_date createdAt}}.</p>
    </div>
  </a>
{{/each}}